<!doctype html>
<html lang="en">

<head>
    <title>{{ __app_nice_name }}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="X-Clacks-Overhead" content="GNU Terry Pratchett" />
    <link rel="stylesheet" href="/static/zy.css" />
</head>

<body>
    <main>
        <h1 id="TITLE">{{ __app_nice_name }}</h1>
        <table>
            <tr>
                <th>Inventory</th>
                <th>Source URL</th>
            </tr>
        {% for inventory, inventory_dict in inventories.items() %}<tr><td><a href="/inventory/{{ inventory }}">{{ inventory }}</a></td><td><a href="{{ inventory_dict['url'] }}">{{ inventory_dict['url'] }}</a></td></tr>{% endfor %}
    </main>
</body>

<script src="/static/sorttable.js"></script>

</html>
